const Discord = require('discord.js');
 const client = new Discord.Client();

client.on('ready', () => {
 console.log(`Logged in as Kanye`);
 });

client.on('message', msg => {
 if (msg.content === 'Kanye') {
 msg.channel.send('I like boys');
 }
 });

client.on('message', msg => {
 if (msg.content === 'Send amogus') {
 msg.channel.send('https://cdn.discordapp.com/attachments/645265290585702411/861520463405514752/Screenshot_20210702-231507999__01.jpg');
 }
 });

client.on('message', msg => {
 if (msg.content === 'Kanye') {
 msg.channel.send();
 function myFunc(arg) {
  msg.channel.send(`Obama hamburger`);
}


setTimeout(myFunc, 10000, 'funky');
}
 });

